<h1>
Result
</h1>
<% if @results != 0 %>
  <% @results.each do |result| %>
    <% if result.profile %>
      <p>NAME:<%= link_to result.name, profile_path(result.profile.id) %></p>
      <p>TEAM:<%= result.profile.team.name %></p>
      <p>CONTACT:<%= result.profile.contact %></p>
      <p>JOINING DATE:<%= result.profile.joining_date %>
    <% end %>
  <% end %>
<% end %>